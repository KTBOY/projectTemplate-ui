@-webkit-keyframes load {
0% {
    height: 20%;
}
50% {
    height: 100%;
}
100% {
    height: 20%;
}
}
@keyframes load {
0% {
    height: 20%;
}
50% {
    height: 100%;
}
100% {
    height: 20%;
}
}

