<view><view class="calendar"><view class="detail"><view class="datedetail">{{datedetail}}</view><view class="festival">{{festival}}</view></view><view class="calendarhead"><view class="tr"><text>日</text><text>一</text><text>二</text><text>三</text><text>四</text><text>五</text><text>六</text></view></view><swiper class="swiper" style="{{('height:'+sheight+'px')}}" current="{{current}}" circular="true" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiper}}" wx:for-item="sitem" wx:for-index="sindex" wx:key="sindex"><swiper-item><block wx:for="{{normalDateList}}" wx:for-item="index" wx:for-index="i" wx:key="i"><view class="week"><view class="tr"><block wx:for="{{normalDateList[i]}}" wx:for-item="noritem" wx:for-index="idx" wx:key="idx"><block><text data-event-opts="{{[['tap',[['choose',['$0',i],[[['normalDateList.'+i+'','',idx,'text']]]]]]]}}" class="{{[(idx==0||idx==6)?'aorange':'',(noritem.isToday)?'agreen':'']}}" bindtap="__e">{{noritem.text}}</text></block></block></view><view class="tr tr2"><block wx:for="{{lunarDateList[i]}}" wx:for-item="lunitem" wx:for-index="idx" wx:key="idx"><block><text data-event-opts="{{[['tap',[['choose',['$0',i],[[['lunarDateList.'+i+'','',idx,'days']]]]]]]}}" class="{{[(idx==0||idx==6)?'aorange':'',(lunitem.isToday)?'agreen':'']}}" bindtap="__e">{{lunitem.text}}</text></block></block></view></view></block></swiper-item></block></swiper><block wx:if="{{show}}"><view data-event-opts="{{[['tap',[['toggle',['$event']]]]]}}" class="show" bindtap="__e">收</view></block><block wx:else><view data-event-opts="{{[['tap',[['toggle',['$event']]]]]}}" class="show" bindtap="__e">开</view></block></view><view class="panel"><view data-event-opts="{{[['tap',[['changeDate',['YU']]]]]}}" bindtap="__e">上一年↑</view><view data-event-opts="{{[['tap',[['changeDate',['YD']]]]]}}" bindtap="__e">下一年↓</view><view data-event-opts="{{[['tap',[['changeDate',['MU']]]]]}}" bindtap="__e">上一月↑</view><view data-event-opts="{{[['tap',[['changeDate',['MD']]]]]}}" bindtap="__e">下一月↓</view><view data-event-opts="{{[['tap',[['changeDate',['']]]]]}}" bindtap="__e">当前月</view></view></view>